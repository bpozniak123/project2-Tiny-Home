<%- include('./partials/header.ejs') %>
	<h1>Tiny Home Index</h1>
	<ul>
		<% for (let i = 0; i < tinyHome.length; i++) {%>
		<li>
			<img src="<%= tinyHome[i].img%>"><br>
			<a href="/tiny/<%= tinyHome[i].id %>"> <%= tinyHome[i].type %> </a><br>
		
				"<%= tinyHome[i].description %>"<br>
		</li>
		<a href="/tiny/<%= tinyHome[i].id %>/edit">EDIT</a><br>
	
	<form action="/tiny/<%= tinyHome[i].id %>?_method=DELETE" method="POST">
		<input type="submit" value="Delete" name="type">
	</form>
		<% } %>
	</ul>
<%- include('./partials/footer.ejs') %>